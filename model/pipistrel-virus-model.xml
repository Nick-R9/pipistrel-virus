<?xml version="1.0"?>

<PropertyList>

  <path>pipistrel-virus.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10000</max-m>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>prop slow</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>-1</factor>
    <center>
      <x-m>3.01</x-m>
      <y-m>0</y-m>
      <z-m>0.113</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>

</PropertyList>
